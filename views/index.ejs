<!DOCTYPE html>
<html>
<head>
	<title>Фан клуб Арсенал</title>
	<%-include('link')%>
</head>
<meta charset="utf-8">
<body>
	<%-include('header')%>
	<center>
	<div class="informacion">
		<!-- <img src="images/w.JPG"> -->
		<div class="owl-carousel">
			<%for(i in result.slider){%>
			 <img src="images/<%-result.slider[i].img%>">
			 <%}%>
		</div>
		<div class="news">
			<%for(i in result.news){%>
				<div class="item">
					<img src="images/<%-result.news[i].img%>">
					<div class="content">
						<p><%-result.news[i].title%></p>
						<h4><%-result.news[i].lowContent%></h4>
					</div>
				</div>
			<%}%>
			</div>
		</div>
	</div>
</center>
<%-include('footer')%>
</body>
</html>